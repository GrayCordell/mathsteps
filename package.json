{
  "name": "mathsteps-expermental-fork",
  "description": "Step by step math solutions. Experimental Fork",
  "version": "0.0.1",
  "type": "module",
  "private": false,
  "packageManager": "pnpm@8.15.4",
  "exports": {
    ".": {
      "import": "./dist/math-steps-expermental-fork.js",
      "require": "./dist/math-steps-expermental-fork.cjs"
    }
  },
  "main": "./dist/math-steps-expermental-fork.umd.cjs",
  "module": "./dist/math-steps-expermental-fork.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "dev": "vite --port 3333",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "lint": "eslint .",
    "preview-https": "serve dist",
    "test": "vitest",
    "test:e2e": "cypress open",
    "test:unit": "vitest",
    "test:watch": "vitest",
    "test:coverage": "vitest --coverage",
    "typecheck": "vue-tsc --noEmit",
    "up": "taze major -I",
    "sizecheck": "npx vite-bundle-visualizer",
    "cleanFix": "rd /s /q node_modules & del /q package-lock.json & del /q pnpm-lock.yaml && pnpm install"
  },
  "keywords": [
    "math",
    "steps",
    "algebra",
    "cas",
    "computer",
    "algebra",
    "system"
  ],
  "license": "Apache-2.0",
  "author": "Grayson Cordell",
  "dependencies": {
    "globals": "^13.24.0",
    "mathjs": "^12.4.0"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*": "eslint --fix"
  },
  "devDependencies": {
    "lint-staged": "^15.2.2",
    "@antfu/eslint-config": "^2.6.4",
    "@types/node": "^20.11.17",
    "@typescript-eslint/eslint-plugin": "^6.2.0",
    "@typescript-eslint/parser": "^6.2.0",
    "@vitest/coverage-v8": "^1.3.1",
    "@vue/test-utils": "^2.4.4",
    "cross-env": "^7.0.3",
    "eslint": "^8.57.0",
    "eslint-plugin-format": "^0.1.0",
    "https-localhost": "^4.7.1",
    "camelcase": "^8.0.0",
    "jsdom": "^24.0.0",
    "pnpm": "^8.15.4",
    "rollup": "^4.12.0",
    "taze": "^0.13.3",
    "typescript": "^5.3.3",
    "vite": "^5.1.4",
    "vite-bundle-visualizer": "^1.0.1",
    "vite-plugin-dts": "^3.7.3",
    "vite-plugin-inspect": "^0.8.3",
    "vitest": "^1.3.1",
    "vue-tsc": "^1.8.27"
  }
}

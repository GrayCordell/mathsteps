{
  "name": "mathsteps-experimental-fork",
  "description": "Step by step math solutions. Experimental Fork",
  "version": "0.9.2",
  "type": "module",
  "private": false,
  "engines": {
    "pnpm": ">=9.7.0"
  },
  "packageManager": "pnpm@9.7.0",
  "readme": "./README.md",
  "exports": {
    ".": {
      "types": "./dist/types/mathsteps-experimental-fork.d.ts",
      "import": "./dist/es/lib/index.js",
      "require": "./dist/cjs/lib/index.js"
    }
  },
  "main": "dist/cjs/lib/index.js",
  "module": "dist/es/lib/index.js",
  "types": "dist/types/mathsteps-experimental-fork.d.ts",
  "files": [
    "dist",
    "LICENSE",
    "README.md",
    "CHANGELOG.md"
  ],
  "scripts": {
    "temp": "temp",
    "preinstall": "npx only-allow pnpm",
    "dev": "vite --port 3333",
    "build": "vite build --emptyOutDir",
    "prepublishOnly": "pnpm build",
    "release": "pnpm build && bumpp --no-push && npm publish",
    "preview": "vite preview",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "preview-https": "serve dist",
    "test": "vitest",
    "quickTest": "cross-env VITEST_EXCLUDE='src/tests/skip-this-file.spec.ts,src/tests/slow-tests/*.spec.ts' vitest",
    "test:e2e": "cypress open",
    "test:unit": "vitest",
    "test:watch": "vitest",
    "test:coverage": "vitest --coverage",
    "typecheck": "tsc --noEmit",
    "up": "taze major -I",
    "sizecheck": "npx vite-bundle-visualizer",
    "cleanFix": "rd /s /q node_modules & del /q package-lock.json & del /q pnpm-lock.yaml && pnpm install"
  },
  "keywords": [
    "math",
    "steps",
    "algebra",
    "cas",
    "computer",
    "algebra",
    "system",
    "assess",
    "evaluate"
  ],
  "license": "Apache-2.0",
  "author": "Grayson Cordell",
  "dependencies": {
    "lru-cache": "^11.0.2",
    "mathjs": "^13.1.1"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*": "eslint --fix"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^3.6.0",
    "@types/node": "^22.5.4",
    "@typescript-eslint/eslint-plugin": "^8.5.0",
    "@typescript-eslint/parser": "^8.5.0",
    "@vitest/coverage-v8": "^2.1.1",
    "@vue/test-utils": "^2.4.6",
    "camelcase": "^8.0.0",
    "cross-env": "^7.0.3",
    "eslint": "^9.10.0",
    "eslint-plugin-format": "^0.1.2",
    "https-localhost": "^4.7.1",
    "jsdom": "^25.0.0",
    "lint-staged": "^15.2.10",
    "pnpm": "^9.10.0",
    "rollup": "^4.22.4",
    "taze": "^0.16.8",
    "type-fest": "^4.26.1",
    "typescript": "^5.6.2",
    "vite": "^5.4.6",
    "vite-bundle-visualizer": "^1.2.1",
    "vite-plugin-dts": "^4.2.1",
    "vite-plugin-inspect": "^0.8.7",
    "vitest": "^2.1.1",
    "eslint-plugin-scoped-construct": "^0.2.1",
    "bumpp": "^9.5.2",
    "esno": "^4.7.0"
  }
}

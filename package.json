{
  "name": "mathsteps-expermental-fork",
  "description": "Step by step math solutions. Experimental Fork",
  "version": "0.4.4",
  "type": "module",
  "private": false,
  "packageManager": "pnpm@9.7.0",
  "exports": {
    ".": {
      "types": "./dist/types/mathsteps-expermental-fork.d.ts",
      "import": "./dist/es/lib/index.js",
      "require": "./dist/cjs/lib/index.js"
    }
  },
  "main": "dist/cjs/lib/index.js",
  "module": "dist/es/lib/index.js",
  "types": "dist/types/mathsteps-expermental-fork.d.ts",
  "files": [
    "dist",
    "LICENSE",
    "README.md",
    "CHANGELOG.md"
  ],
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "dev": "vite --port 3333",
    "build": "vite build",
    "preview": "vite preview",
    "lint": "eslint .",
    "preview-https": "serve dist",
    "test": "vitest",
    "test:e2e": "cypress open",
    "test:unit": "vitest",
    "test:watch": "vitest",
    "test:coverage": "vitest --coverage",
    "typecheck": "vue-tsc --noEmit",
    "up": "taze major -I",
    "sizecheck": "npx vite-bundle-visualizer",
    "cleanFix": "rd /s /q node_modules & del /q package-lock.json & del /q pnpm-lock.yaml && pnpm install"
  },
  "keywords": [
    "math",
    "steps",
    "algebra",
    "cas",
    "computer",
    "algebra",
    "system",
    "assess",
    "evaluate"
  ],
  "license": "Apache-2.0",
  "author": "Grayson Cordell",
  "dependencies": {
    "mathjs": "^13.0.3"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*": "eslint --fix"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.25.0",
    "@types/node": "^22.1.0",
    "@typescript-eslint/eslint-plugin": "^8.0.1",
    "@typescript-eslint/parser": "^8.0.1",
    "@vitest/coverage-v8": "^2.0.5",
    "@vue/test-utils": "^2.4.6",
    "camelcase": "^8.0.0",
    "cross-env": "^7.0.3",
    "eslint": "^9.8.0",
    "eslint-plugin-format": "^0.1.2",
    "https-localhost": "^4.7.1",
    "jsdom": "^24.1.1",
    "lint-staged": "^15.2.8",
    "pnpm": "^9.7.0",
    "rollup": "^4.20.0",
    "taze": "^0.16.3",
    "type-fest": "^4.25.0",
    "typescript": "^5.5.4",
    "vite": "^5.4.0",
    "vite-bundle-visualizer": "^1.2.1",
    "vite-plugin-dts": "^4.1.0",
    "vite-plugin-inspect": "^0.8.5",
    "vitest": "^2.0.5"
  }
}
